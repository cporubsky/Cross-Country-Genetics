  <div class="page-header text-center" style="margin-top: 10px">
  		<h1 id="heading">Cross Country Genetics</h1>
  </div>
  <% if(sessionFlash && sessionFlash.message) { %>
    <div class="alert alert-<%= sessionFlash.type %> alert-dismissible fade in" role="alert">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <%= sessionFlash.message %>
    </div>
  <% } else if (message) { %>
    <div class="alert alert-danger alert-dismissible fade in" role="alert">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <%= message %>
    </div>
  <% } %>

