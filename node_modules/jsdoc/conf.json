{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ "app.js", "db.js", "endpoints", "database" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
      "encoding": "utf8",
      "recurse": true
    }
}
